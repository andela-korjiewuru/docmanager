{
  "name": "docmanager",
  "version": "1.0.0",
  "description": "A beautiful document management system specially made for You!",
  "main": "server.js",
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "start": "npm run start:dev:server",
    "migrate:test": "NODE_ENV=test sequelize db:migrate",
    "undo:migrate:test": "NODE_ENV=test sequelize db:migrate:undo:all",
    "migrate": "cross-env NODE_ENV=development sequelize db:migrate",
    "undo:migrate": "NODE_ENV=development sequelize db:migrate:undo:all",
    "start:dev": "npm-run-all --parallel start:dev:server lint:watch",
    "start:dev:server": "./node_modules/.bin/babel-node -- \"./src/bin/www\"",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "seed:test-db": "NODE_ENV=test babel-node ./src/server/tests/seeders/dbSeeders",
    "seed-db": "babel-node ./src/server/tests/seeders/dbSeeders",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "server-side-test": "NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=html mocha \"src/server/tests/**/*.spec.js\" --timeout 10000 && npm run report-coverage",
    "frontend-test": "NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=html mocha --require \"src/client/tests/setup.js\" --recursive \"src/client/tests/**/*.spec.js\"",
    "test": "npm run seed:test-db && npm run server-side-test",
    "e2e": "",
    "test:watch": "npm run test -- --watch",
    "webpack-fix": "webpack --progress --profile --colors --display-error-details --display-cached"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-korjiewuru/docmanager.git"
  },
  "bugs": {
    "url": "https://github.com/andela-korjiewuru/docmanager/issues"
  },
  "homepage": "https://github.com/andela-korjiewuru/docmanager#readme",
  "author": "Orjiewuru Kingdom Isaac",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.8.0",
    "babel-polyfill": "6.8.0",
    "babel-register": "6.8.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.1",
    "bootstrap": "3.3.6",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "faker": "^4.1.0",
    "jquery": "2.2.3",
    "jsdom": "8.5.0",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "materialize-css": "^0.98.2",
    "morgan": "^1.8.1",
    "open": "0.0.5",
    "path": "^0.12.7",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-hot-loader": "^1.3.1",
    "react-materialize": "^0.18.4",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "react-tinymce": "^0.5.1",
    "redux": "3.5.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable-state-invariant": "^1.2.3",
    "redux-thunk": "2.0.1",
    "sequelize": "^3.30.4",
    "toastr": "2.1.2",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "coveralls": "^2.13.1",
    "cross-env": "^1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsdoc": "^2.4.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "jsdom-global": "3.0.2",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "nyc": "^10.3.2",
    "react-addons-test-utils": "^15.0.2",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "rimraf": "2.5.2",
    "should": "^11.2.1",
    "sinon": "^2.3.2",
    "style-loader": "0.13.1",
    "supertest": "^3.0.0",
    "url-loader": "0.5.7"
  }
}
